function realDelete(t){confirm("你确定删除订单吗？")&&$.ajax({type:"POST",dataType:"text",url:"pcOrder_realDelete.action",data:"orderId="+t,success:function(e){try{var r=$.parseJSON(e);if("y"==r.status){JM.alert("删除成功",2e3);for(var a=$("#tbc2_01").find("input"),n=$("#tbc2_02").find("input"),i=$("#tbc2_03").find("input"),o=$("#tbc2_04").find("input"),d=$("#tbc2_05").find("input"),p=0;p<a.length;p++)t==$(a[p]).attr("id")&&$("#"+t).parent().parent().remove();for(var p=0;p<n.length;p++)t==$(n[p]).attr("id")&&$("#"+t).parent().parent().remove();for(var p=0;p<i.length;p++)t==$(i[p]).attr("id")&&$("#"+t).parent().parent().remove();for(var p=0;p<o.length;p++)t==$(o[p]).attr("id")&&$("#"+t).parent().parent().remove();for(var p=0;p<d.length;p++)t==$(d[p]).attr("id")&&$("#"+t).parent().parent().remove()}else JM.alert(r.info,3e3)}catch(t){}}})}function orderInfo(t){JM.goUrl("pcOrder_orderDetailsPage.action?orderId="+t)}